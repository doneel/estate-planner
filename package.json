{
  "name": "estate-planner-3215",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS='-r ./esbuild-overrides' run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='-r ./esbuild-overrides' run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "printWidth": 200
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@fullstory/browser": "^1.6.2",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/server-runtime": "^1.7.2",
    "@stytch/nextjs": "^0.5.0",
    "@stytch/react": "^0.5.0",
    "@stytch/vanilla-js": "^0.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@turf/turf": "^6.5.0",
    "@types/ol": "^7.0.0",
    "@types/react-scroll": "^1.8.4",
    "@types/uuid": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.2.0",
    "gojs": "^2.3.3",
    "googleapis": "^109.0.1",
    "isbot": "^3.5.3",
    "ol": "^7.2.2",
    "ol-ext": "^4.0.6",
    "react": "^18.2.0",
    "react-currency-format": "^1.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scroll": "^1.8.8",
    "rlayers": "^1.4.8",
    "stytch": "^5.15.0",
    "tiny-invariant": "^1.2.0",
    "ts-custom-error": "^3.3.1",
    "typescript-json-serializer": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@faker-js/faker": "^7.5.0",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.18",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.0.9",
    "@types/react": "^18.0.20",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.11",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.8.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
